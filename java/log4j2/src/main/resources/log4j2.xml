<?xml version="1.0" encoding="UTF-8"?>

<Configuration status="warn" strict="true">
    <Appenders>
        <Console name="console">
            <GoogleCloudLoggingJsonLayout />
        </Console>
    </Appenders>
    <Loggers>
        <Root level="info" includeLocation="true">
            <AppenderRef ref="console"/>
        </Root>
    </Loggers>
</Configuration>